    "release:android:apk": "yarn android-fix && cd android && ./gradlew clean && ./gradlew assembleRelease && cd .. && open .",
    "build-android-store": "yarn android-fix && cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "set-icon-ios": "react-native set-icon --platform ios --path src/assets/images/logo1024_pocket.png",
