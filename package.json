    "adb": "adb reverse tcp:8081 tcp:8081",
    "pre-ios": "cd ios && pod install && cd ..",
    "dev": "yarn start --reset-cache",
    "initTheme": "cp -r src/themes BaseComponent/themes",
    "android-fix": "npx jetify",
    "build-android": "yarn android-fix && cd android && ./gradlew clean && cd .. && yarn android",
